---
import { toStaticPath } from './blog-routing';
import type { Post } from './blog-routing'; 

export async function getStaticPaths() {
  const posts = await Astro.glob<Post>('../_blog/*.{md,mdx}');
  return posts.map(toStaticPath);
}

const { post } = Astro.props;
const { Content } = post;
---
<Content />
